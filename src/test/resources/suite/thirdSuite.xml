<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="all-tests" verbose="1">
    <test name="All Tests">
        <packages>
            <package name=".*"/>
        </packages>
    </test>
    <groups>
        <run>
            <exclude name="exceptions"  />
        </run>
    </groups>
</suite>